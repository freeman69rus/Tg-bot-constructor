{
  "version": "1.0.0",
  "compilerOptions": {
    "outDir": "./dist",
    "lib": [
      "es6",
      "es2017",
      "dom",
      "es2015.promise"
    ],
    "target": "es2017",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["./**/*"],
  "exclude": [
  ]
}
